<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.SearchViewController">
   <children>
      <ScrollPane layoutX="520.0" layoutY="-1.0">
         <content>
            <ListView fx:id="listView" onMouseClicked="#programClickedOn" />
         </content>
      </ScrollPane>
      <TextArea fx:id="creditArea" layoutX="13.0" layoutY="184.0" prefHeight="203.0" prefWidth="490.0" />
      <Accordion />
      <TextField fx:id="searchField" layoutX="14.0" layoutY="55.0" prefHeight="25.0" prefWidth="386.0" />
      <Button fx:id="searchBtn" layoutX="407.0" layoutY="54.0" mnemonicParsing="false" onAction="#Search" prefHeight="26.0" prefWidth="46.0" text="Søg">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="logOutbtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitButtonPushed" prefHeight="26.0" prefWidth="73.0" text="Afslut">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label layoutX="113.0" layoutY="21.0" text="Indtast navnet på produktionen">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
